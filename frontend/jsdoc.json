{
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules|dist)"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md",
    "template": "default"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}